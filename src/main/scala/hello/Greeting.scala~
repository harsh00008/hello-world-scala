package hello

class Greeting{
	private[scope] final long def id
	private[scope] final String def content
	
	def Greeting(args: ){
	
	}

	def getId(): long{

	}
	
	def getContent(): String{

	}
}


public class Greeting {

    private final long id;
    private final String content;

    public Greeting(long id, String content) {
        this.id = id;
        this.content = content;
    }

    public long getId() {
        return id;
    }

    public String getContent() {
        return content;
    }
}

